lớp  công khai bai1 {
    // Tập tin bai1.java

// Định nghĩa gói "myPackage" chứa các lớp liên quan đến hình học
gói  myPackage ;

// Lớp HinhVuong biểu diễn hình vuông
public  class  HinhVuong {
    // Thuộc tính canh biểu tượng cạnh của hình vuông
     canh đôi  riêng ;

    // Phương thức khởi tạo với tham số cận
    công  HinhVuong ( double  canh ) {
        này . canh = canh ;
    }

    // Phương thức getter trả về giá trị của thuộc tính canh
    công  đôi  getCanh () {
        quay  canh ;
    }

    // Phương thức thiết lập giá trị của thuộc tính canh
    public  void  setCanh ( double  canh ) {
        này . canh = canh ;
    }

    // Phương thức tính diện tích của hình vuông
    public  double  tinhDienTich () {
        trả  canh * canh ;
        
    }
}

// Lớp HinhTron biểu diễn hình tròn
public  class  HinhTron {
    // Thuộc tính banKinh biểu diễn bán kính của hình tròn
    riêng  đôi  banKinh ;

    // Phương thức khởi tạo với tham số bán kính
    public  HinhTron ( double  banKinh ) {
        này . banKinh = banKinh ;
    }

    // Phương thức getter trả về giá trị của thuộc tính banKinh
    công  đôi  getBanKinh () {
        trả lại  bảnkinh ;
    }

    // Phương thức setter setting value of property banKinh
    public  void  setBanKinh ( double  banKinh ) {
        này . banKinh = banKinh ;
    }

    // Phương thức tính diện tích của hình tròn
    public  double  tinhDienTich () {
        trả lại  Toán học . PI * banKinh * banKinh ;
    }
}

    
}
